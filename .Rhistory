library(knitr)
age <- 0:4
sx <- c(740,280,105,32,0)
bx <- c(0,0.4,1.3,3.8,NA)
lx <- sx/740
px <- vector(mode = 'numeric',length = 5)
for(i in 1:length(lx)){
px[i] <- (lx[i+1]/lx[i])
}
lifetable <- cbind(age,sx,lx,px,bx)
colnames(lifetable) <- c('Age','S_x','l_x','p_x','b_x')
lifetable <- as.data.frame(lifetable)
kable(lifetable)
